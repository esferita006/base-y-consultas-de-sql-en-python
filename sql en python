!pip install db-sqlite3

import sqlite3

# Connect to SQLite database (or create a new one if it doesn't exist)
conn = sqlite3.connect('your_database_name.db')
cursor = conn.cursor()

# Execute the SQL statements to create tables
cursor.executescript('''
CREATE TABLE IF NOT EXISTS alumno (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    nombre TEXT NOT NULL,
    edad INTEGER NOT NULL,
    sexo TEXT NOT NULL,
    entidad TEXT NOT NULL,
    ocupacion TEXT NOT NULL
);

CREATE TABLE IF NOT EXISTS RegistroMarcas (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    MarcaTemporal TEXT
);

CREATE TABLE IF NOT EXISTS Consentimientos (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    ConsentimientoTexto TEXT
);

CREATE TABLE IF NOT EXISTS IntervaloEdad (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    RangoEdad TEXT
);

CREATE TABLE IF NOT EXISTS SexoNacimiento (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    Sexo TEXT
);

CREATE TABLE IF NOT EXISTS AreaLaboral (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    Area TEXT
);

CREATE TABLE IF NOT EXISTS EntidadNacimiento (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    Entidad TEXT
);

CREATE TABLE IF NOT EXISTS AsignaturasCursadas (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    Asignatura TEXT
);

CREATE TABLE IF NOT EXISTS ModalidadEnsenanza (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    Modalidad TEXT
);

CREATE TABLE IF NOT EXISTS UsoGrabaciones (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    UsoConsulta TEXT
);

CREATE TABLE IF NOT EXISTS PlataformasTrabajo (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    Plataforma TEXT
);

CREATE TABLE IF NOT EXISTS EficaciaAlumno (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    NivelEficacia INTEGER,
    ComunicacionProfesores INTEGER,
    AumentoInteresContenido INTEGER,
    MejoraMemoria INTEGER,
    MejoraAtencion INTEGER,
    PersonalizacionAprendizaje INTEGER,
    InversionCapacitacion INTEGER,
    MejoraRetroalimentacion INTEGER,
    SimplificacionTareasAdmin INTEGER,
    FlexibilidadPlaneacion INTEGER,
    MejoraParticipacionAlumnos INTEGER,
    MejoraAutoevaluacion INTEGER,
    NivelEsfuerzo INTEGER
);

CREATE TABLE IF NOT EXISTS EficaciaProfesor (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    NivelEficacia INTEGER,
    NivelEsfuerzo INTEGER,
    ConocimientosIniciales TEXT,
    ConocimientosNecesarios TEXT,
    ContribucionCurso TEXT,
    UsadoGoogleColab TEXT
);

CREATE TABLE IF NOT EXISTS ConocimientosTecnologicos (
    ID INTEGER PRIMARY KEY AUTOINCREMENT,
    NivelEficacia INTEGER,
    UsoGoogleColab TEXT,
    FacilidadUsoPythonColab TEXT,
    UsoGoogleDrive TEXT,
    PreferenciaClasesColab TEXT,
    BeneficioGrabacionesClase TEXT,
    InteractividadClases TEXT,
    FormatoClasesEnLinea TEXT,
    DistraccionesDuranteClase TEXT,
    ConcentracionDuranteClase TEXT,
    OtrasMateriasHoraClase TEXT,
    FacilidadAprendizajePython TEXT,
    DificultadTemas TEXT,
    ComprensionPOO TEXT,
    UsoIA TEXT,
    TecnicasAprendizaje TEXT,
    AyudaIAAprendizaje TEXT,
    UsoSQLiteColab TEXT
);

